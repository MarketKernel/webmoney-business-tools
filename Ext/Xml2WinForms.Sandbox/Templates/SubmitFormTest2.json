{
  "text": "Send Message To Developer",
  "steps": [
    {
      "tunableShape": {
        "columns": [
          {
            "controls": [
              {
                "type": "NumericUpDown",
                "desc": "NotEmpty NumericUpDown:",
                "inspectionRules": [
                  {
                    "type": "NotEmpty",
                    "message": "NotEmpty!"
                  }
                ]
              },
              {
                "type": "NumericUpDown",
                "desc": "ValueMoreThen 3 NumericUpDown:",
                "inspectionRules": [
                  {
                    "type": "ValueMoreThen",
                    "etalon": 3,
                    "message": "ValueMoreThen 3!"
                  }
                ]
              },
              {
                "type": "NumericUpDown",
                "desc": "ValueLessThen 5 NumericUpDown:",
                "inspectionRules": [
                  {
                    "type": "ValueLessThen",
                    "etalon": 5,
                    "message": "ValueLessThen 5!"
                  }
                ]
              },
              {
                "type": "CheckBox",
                "desc": "Отключить GroupBox",
                "behaviorRules": [
                  {
                    "trigger": "CheckedChanged",
                    "affectedControls": [
                      "GroupBox1"
                    ],
                    "action": "Disable"
                  }
                ]
              },
              {
                "type": "GroupBox",
                "desc": "Client info",
                "name": "GroupBox1",
                "column": {
                  "controls": [
                    {
                      "type": "TextBox",
                      "desc": "NotEmpty + LengthEqual=3:",
                      "inspectionRules": [
                        {
                          "type": "NotEmpty",
                          "message": "NotEmpty!"
                        },
                        {
                          "type": "LengthEqual",
                          "etalon": 3,
                          "message": "LengthEqual!"
                        }
                      ]
                    },
                    {
                      "type": "TextBox",
                      "desc": "Email regExp:",
                      "inspectionRules": [
                        {
                          "type": "RegExp",
                          "RegExp": "^([\\w\\.\\-]+)@([\\w\\-]+)((\\.(\\w){2,3})+)$",
                          "message": "RegExp!"
                        }
                      ]
                    },
                    {
                      "type": "TextBox",
                      "desc": "Test:"
                    },
                    {
                      "type": "GroupBox",
                      "desc": "Client info",
                      "column": {
                        "controls": [
                          {
                            "type": "TextBox",
                            "desc": "NotEmpty + LengthEqual=3:",
                            "inspectionRules": [
                              {
                                "type": "NotEmpty",
                                "message": "NotEmpty!"
                              }
                            ]
                          },
                          {
                            "type": "CheckBox",
                            "desc": "NotEmpty CheckBox",
                            "inspectionRules": [
                              {
                                "type": "NotEmpty",
                                "message": "NotEmpty!"
                              }
                            ]
                          },
                          {
                            "type": "ComboBox",
                            "desc": "Authentication type:",
                            "items": [
                            ],
                            "inspectionRules": [
                              {
                                "type": "NotEmpty",
                                "message": "NotEmpty!"
                              }
                            ]
                          },
                          {
                            "type": "DateTimePicker",
                            "desc": "DateTimePicker 1:"
                          }
                        ]
                      }
                    }
                  ]
                }
              }
            ]
          }
        ]
      },
      "actionText": "&Send message"
    }
  ]
}